<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/suit_medium"
        android:text="프로필 수정"
        android:textSize="18dp"
        android:textColor="@color/Gray900"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="318dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:srcCompat="@drawable/ic_left2" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/suit_bold"
        android:text="프로필 사진"
        android:textColor="@color/Gray900"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/imageView10" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView18"
        android:layout_width="84dp"
        android:layout_height="84dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:src="@drawable/imgbed"
        app:civ_border_overlay="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/soldoutlayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toTopOf="@+id/imageView18">

        <ImageView
            android:id="@+id/soldoutback"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:alpha="0.6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.444"
            app:srcCompat="@drawable/background_circle" />

        <ImageView
            android:id="@+id/soldout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_image" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/profiletext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/suit_bold"
        android:text="닉네임"
        android:textColor="@color/Gray900"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/imageView18" />

    <TextView
        android:id="@+id/textID"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/anotherprofileroudradius"
        android:fontFamily="@font/suit_semibold"
        android:gravity="center|start"
        android:paddingLeft="12dp"
        android:text="숭실공인중개사사무소"
        android:textColor="@color/Gray900"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/profiletext"
        app:layout_constraintTop_toBottomOf="@+id/profiletext" />

    <TextView
        android:id="@+id/profiletext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/suit_bold"
        android:text="아이디"
        android:textColor="@color/Gray900"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textID" />

    <TextView
        android:id="@+id/textID2"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/anotherprofileroudradius"
        android:gravity="center|start"
        android:paddingLeft="12dp"
        android:fontFamily="@font/suit_semibold"
        android:text="24zip@gmail.com"
        android:textColor="@color/Gray600"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/profiletext2"
        app:layout_constraintTop_toBottomOf="@+id/profiletext2" />

    <TextView
        android:id="@+id/profiletext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/suit_bold"
        android:text="비밀번호"
        android:textColor="@color/Gray900"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textID2" />

    <EditText
        android:id="@+id/textPassword"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/anotherprofileroudradius"
        android:gravity="center|start"
        android:inputType="textPassword"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:fontFamily="@font/suit_semibold"
        android:text="123456"
        android:textColor="@color/Gray900"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/profiletext3"
        app:layout_constraintTop_toBottomOf="@+id/profiletext3" />

    <EditText
        android:id="@+id/textPasswordcheck"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/anotherprofileroudradius"
        android:fontFamily="@font/suit_semibold"
        android:gravity="center|start"
        android:inputType="textPassword"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:hint="비밀번호 확인"
        android:textColor="@color/Gray900"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/profiletext3"
        app:layout_constraintTop_toBottomOf="@+id/textPassword" />

    <TextView
        android:id="@+id/textchecking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text=" 비밀번호 조건이 일치하지 않아요"
        android:drawableLeft="@drawable/ic_alert_red"
        android:fontFamily="@font/suit_regular"
        android:textColor="@color/Error"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/profiletext3"
        app:layout_constraintStart_toEndOf="@+id/profiletext3"
        app:layout_constraintTop_toTopOf="@+id/profiletext3" />

    <android.widget.Button
        android:id="@+id/button2"
        android:layout_width="324dp"
        android:layout_height="56dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/profileroudradius_gray"
        android:fontFamily="@font/suit_semibold"
        android:text="수정완료"
        android:textColor="@color/Gray900"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textPassword"
        app:layout_constraintEnd_toEndOf="@+id/textPassword"
        app:layout_constraintTop_toTopOf="@+id/textPassword"
        app:srcCompat="@drawable/eye_off" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textPasswordcheck"
        app:layout_constraintEnd_toEndOf="@+id/textPasswordcheck"
        app:layout_constraintTop_toTopOf="@+id/textPasswordcheck"
        app:srcCompat="@drawable/eye_off" />

</androidx.constraintlayout.widget.ConstraintLayout>