<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/search_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:textStyle="bold"
        android:text="회원가입"
        android:textSize="18dp"
        android:textColor="@color/Gray900"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="104dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:srcCompat="@drawable/ic_left2" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="324dp"
        android:layout_height="54dp"
        android:layout_marginStart="18dp"
        android:fontFamily="@font/suit_variable"
        android:gravity="top"
        android:lineHeight="27sp"
        android:lineSpacingExtra="5sp"
        android:text="회원가입에 필요한 정보를\n기입해주세요."
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        android:translationY="-2.27sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/search_toolbar" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout1_1"
        android:layout_width="188dp"
        android:layout_height="24dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="@+id/some_id"
        app:layout_constraintTop_toBottomOf="@+id/some_id">

        <TextView
            android:id="@+id/some_id2"
            android:layout_width="43dp"
            android:layout_height="24dp"
            android:fontFamily="@font/suit_variable"
            android:gravity="top"
            android:lineHeight="24sp"
            android:lineSpacingExtra="5sp"
            android:text="아이디"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="-2.27sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>


    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_below="@+id/layout1_1"
        android:background="@drawable/contents"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/layout1_1"
        app:layout_constraintTop_toBottomOf="@+id/layout1_1">

        <TextView
            android:id="@+id/some_id1_1"
            android:layout_width="136dp"
            android:layout_height="18dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/suit_variable"
            android:gravity="top"
            android:lineHeight="18sp"
            android:lineSpacingExtra="3sp"
            android:text="이메일 주소를 입력해 주세요"
            android:textColor="#5A5A5A"
            android:textSize="12sp"
            android:translationY="-1.51sp" />

    </RelativeLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/thin_line"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/some_id3"
        android:layout_width="57dp"
        android:layout_height="24dp"
        android:fontFamily="@font/suit_variable"
        android:gravity="top"
        android:lineHeight="24sp"
        android:lineSpacingExtra="5sp"
        android:text="비밀번호"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        android:translationY="-2.27sp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="@+id/relativeLayout"
        app:layout_constraintTop_toBottomOf="@+id/line2" />

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_below="@+id/layout1_1"
        android:background="@drawable/contents"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/some_id3"
        app:layout_constraintTop_toBottomOf="@+id/some_id3">

        <TextView
            android:id="@+id/some_id1_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/suit_variable"
            android:gravity="top"
            android:lineHeight="18sp"
            android:lineSpacingExtra="3sp"
            android:text="8자리 이상 영문, 숫자, 특수문자 포함"
            android:textColor="#5A5A5A"
            android:textSize="12sp"
            android:translationY="-1.51sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_below="@+id/layout1_1"
        android:background="@drawable/contents"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/relativeLayout2"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2">

        <TextView
            android:id="@+id/some_id1_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/suit_variable"
            android:gravity="top"
            android:lineHeight="18sp"
            android:lineSpacingExtra="3sp"
            android:text="비밀번호 확인"
            android:textColor="#5A5A5A"
            android:textSize="12sp"
            android:translationY="-1.51sp" />

    </RelativeLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@drawable/thin_line"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout3" />


    <TextView
        android:id="@+id/some_id4"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:fontFamily="@font/suit_variable"
        android:gravity="top"
        android:lineHeight="24sp"
        android:lineSpacingExtra="5sp"
        android:text="닉네임"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginTop="28dp"
        android:translationY="-2.27sp"
        app:layout_constraintStart_toStartOf="@+id/relativeLayout3"
        app:layout_constraintTop_toBottomOf="@+id/line3" />

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_below="@+id/some_id4"
        android:background="@drawable/contents"
        android:layout_marginTop="8dp"

        app:layout_constraintStart_toStartOf="@+id/some_id4"
        app:layout_constraintTop_toBottomOf="@+id/some_id4">

        <TextView
            android:id="@+id/some_id1_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/suit_variable"
            android:gravity="top"
            android:lineHeight="18sp"
            android:lineSpacingExtra="3sp"
            android:text="닉네임을 입력해주세요"
            android:textColor="#5A5A5A"
            android:textSize="12sp"
            android:translationY="-1.51sp" />

    </RelativeLayout>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frame_10000_background"
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:background="@drawable/frame_10000_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="12dp"
            android:background="#00FFFFFF"
            android:contentDescription="Register Button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/frame_10000_background"
            app:srcCompat="@drawable/btn_sign_up_setting_gray" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>